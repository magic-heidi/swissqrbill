    "@schoero/eslint-config": "^1.4.0",
    "@types/blob-stream": "^0.1.30",
    "@types/node": "^17.0.23",
    "@types/svg-parser": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "assert": "^2.0.0",
    "brfs": "^2.0.2",
    "browserify-zlib": "^0.2.0",
