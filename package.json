    "readable-stream": "^3.6.0",
    "transform-loader": "^0.2.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "util": "^0.12.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/pdfkit": "^0.12.3",
    "blob-stream": "^0.1.3",
    "pdfkit": "^0.13.0",
    "svg-engine": "^0.2.3",
    "svgpath": "^2.5.0"
  }
}
